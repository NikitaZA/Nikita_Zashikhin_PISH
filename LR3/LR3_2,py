matrix = [[1, 2, 3],
          [4, 5, 6],
          [7, 8, 9]]
shift = 1
rotated_matrix = []

if matrix and matrix[0]:
       shift = shift % len(matrix[0])

for row in matrix:
        shifted_row = row[-shift:] + row[:-shift]
        rotated_matrix.append(shifted_row)

print(rotated_matrix)